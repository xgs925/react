{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "jsx": true,
      "arrowFunctions": true,
      "classes": true,
      "modules": true,
      "defaultParams": true
    },
    "sourceType": "module"
  },
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "rules": {
    "indent": ["error", 4, {"SwitchCase": 1}],
    "linebreak-style": ["off", "windows"],
    "quotes": ["error", "single"],
    "semi": ["error", "always"],
    "react/jsx-uses-react": "error",
    "react/jsx-uses-vars": "error",
    "prefer-const": 1,
    "key-spacing": [2, { "beforeColon": false, "afterColon": true }],
    "curly": [2, "all"],
    "eqeqeq": 2,
    "eol-last": 0,
    "comma-spacing": 1,
    "comma-dangle": [1, "never"],
    "no-spaced-func": 2,
    "no-console": 0,
    "no-empty": 1,
    "no-undef":2,
    "no-unused-vars":1,
    "arrow-parens": "error"
  },
  "globals": {
    "window": true,
    "document": true,
    "alert": true,
    "FileReader": true,
    "localStorage": true,
    "fetch": true,
    "confirm": true,
    "FormData": true,
    "navigator": true,
    "XMLHttpRequest": true,
    "console": true,
    "$": true,
    "Swiper": true,
    "process":true,
    "require":true,
    "module":true,
    "JS_ENV": true
  }
}